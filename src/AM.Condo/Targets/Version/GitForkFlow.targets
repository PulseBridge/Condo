<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <NextReleaseBranch  Condition=" '$(NextReleaseBranch)'  == '' "></NextReleaseBranch>
    <ProductionReleaseBranch   Condition=" '$(ProductionReleaseBranch)'   == '' ">master</ProductionReleaseBranch>

    <FeatureBranchPrefix  Condition=" '$(FeatureBranchPrefix)'    == '' "></FeatureBranchPrefix>
    <BugfixBranchPrefix   Condition=" '$(BugfixBranchPrefix)'     == '' "></BugfixBranchPrefix>
    <ReleaseBranchPrefix  Condition=" '$(ReleaseBranchPrefix)'    == '' "></ReleaseBranchPrefix>
    <SupportBranchPrefix  Condition=" '$(SupportBranchPrefix)'    == '' ">lts</SupportBranchPrefix>
    <HotfixBranchPrefix   Condition=" '$(HotfixBranchPrefix)'     == '' "></HotfixBranchPrefix>

    <DefaultBuildQuality        Condition=" '$(DefaultBuildQuality)'       == '' ">alpha</DefaultBuildQuality>

    <NextReleaseBranchBuildQuality  Condition=" '$(NextReleaseBranchBuildQuality)' == '' ">beta</NextReleaseBranchBuildQuality>
    <ProductionReleaseBranchBuildQuality   Condition=" '$(ProductionReleaseBranchBuildQuality)'  == '' "></ProductionReleaseBranchBuildQuality>

    <FeatureBranchBuildQuality  Condition=" '$(FeatureBranchBuildQuality)' == '' ">$(DefaultBuildQuality)</FeatureBranchBuildQuality>
    <BugfixBranchBuildQuality   Condition=" '$(BugfixBranchBuildQuality)'  == '' ">$(DefaultBuildQuality)</BugfixBranchBuildQuality>
    <ReleaseBranchBuildQuality  Condition=" '$(ReleaseBranchBuildQuality)' == '' "></ReleaseBranchBuildQuality>
    <SupportBranchBuildQuality  Condition=" '$(SupportBranchBuildQuality)' == '' ">$(ProductionReleaseBranchBuildQuality)</SupportBranchBuildQuality>
    <HotfixBranchBuildQuality   Condition=" '$(HotfixBranchBuildQuality)'  == '' "></HotfixBranchBuildQuality>

    <VersionTagPrefix Condition=" '$(VersionTagPrefix)' == '' "></VersionTagPrefix>
  </PropertyGroup>
</Project>
