<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <NextReleaseBranch  Condition=" '$(NextReleaseBranch)'  == '' ">develop</NextReleaseBranch>
    <ProductionReleaseBranch   Condition=" '$(ProductionReleaseBranch)'   == '' ">master</ProductionReleaseBranch>

    <FeatureBranchPrefix  Condition=" '$(FeatureBranchPrefix)'    == '' ">feature</FeatureBranchPrefix>
    <BugfixBranchPrefix   Condition=" '$(BugfixBranchPrefix)'     == '' ">bugfix</BugfixBranchPrefix>
    <ReleaseBranchPrefix  Condition=" '$(ReleaseBranchPrefix)'    == '' ">release</ReleaseBranchPrefix>
    <SupportBranchPrefix  Condition=" '$(SupportBranchPrefix)'    == '' ">support</SupportBranchPrefix>
    <HotfixBranchPrefix   Condition=" '$(HotfixBranchPrefix)'     == '' ">hotfix</HotfixBranchPrefix>

    <DefaultBuildQuality        Condition=" '$(DefaultBuildQuality)'       == '' ">alpha</DefaultBuildQuality>

    <NextReleaseBranchBuildQuality  Condition=" '$(NextReleaseBranchBuildQuality)' == '' ">beta</NextReleaseBranchBuildQuality>
    <ProductionReleaseBranchBuildQuality   Condition=" '$(ProductionReleaseBranchBuildQuality)'  == '' "></ProductionReleaseBranchBuildQuality>

    <FeatureBranchBuildQuality  Condition=" '$(FeatureBranchBuildQuality)' == '' ">$(DefaultBuildQuality)</FeatureBranchBuildQuality>
    <BugfixBranchBuildQuality   Condition=" '$(BugfixBranchBuildQuality)'  == '' ">$(DefaultBuildQuality)</BugfixBranchBuildQuality>
    <ReleaseBranchBuildQuality  Condition=" '$(ReleaseBranchBuildQuality)' == '' ">rc</ReleaseBranchBuildQuality>
    <SupportBranchBuildQuality  Condition=" '$(SupportBranchBuildQuality)' == '' ">servicing</SupportBranchBuildQuality>
    <HotfixBranchBuildQuality   Condition=" '$(HotfixBranchBuildQuality)'  == '' ">hotfix</HotfixBranchBuildQuality>

    <VersionTagPrefix Condition=" '$(VersionTagPrefix)' == '' "></VersionTagPrefix>
  </PropertyGroup>
</Project>
