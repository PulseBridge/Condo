{
  "$schema": "http://json.schemastore.org/docfx",
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**/*.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "concepts/**/*.md",
          "concepts/**/toc.yml",
          "tutorials/**/*.md",
          "tutorials/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "overwrite/**.md"
        ],
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "globalMetadataFiles": [
      "global-metadata.json"
    ],
    "fileMetadataFiles": [
      "file-metadata.json"
    ],
    "template": [
      "default"
    ],
    "xref": [
      "https://raw.githubusercontent.com/dotnet/docs/master/_zip/msdnsmallset.yml"
    ],
    "postProcessors": [],
    "noLangKeyword": false
  }
}
